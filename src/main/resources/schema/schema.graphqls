type User {
  id: Int!
  firstName: String!
  lastName: String!
  userName: String!
  password: String!
  address: Address
}

type Address {
    id: Int!
    address1: String!
    address2: String
    city: String!
    state: String!
    postal: String!
}

type Query {
    userById(id: Int!): User
}